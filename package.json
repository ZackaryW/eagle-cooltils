{
  "name": "eagle-cooltils",
  "version": "0.0.2",
  "description": "Utility package for Eagle.cool plugin development",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./universal": {
      "import": {
        "types": "./dist/universal/index.d.ts",
        "default": "./dist/universal/index.js"
      },
      "require": {
        "types": "./dist/universal/index.d.cts",
        "default": "./dist/universal/index.cjs"
      }
    },
    "./win": {
      "import": {
        "types": "./dist/win/index.d.ts",
        "default": "./dist/win/index.js"
      },
      "require": {
        "types": "./dist/win/index.d.cts",
        "default": "./dist/win/index.cjs"
      }
    },
    "./mac": {
      "import": {
        "types": "./dist/mac/index.d.ts",
        "default": "./dist/mac/index.js"
      },
      "require": {
        "types": "./dist/mac/index.d.cts",
        "default": "./dist/mac/index.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/utils/index.d.ts",
        "default": "./dist/utils/index.js"
      },
      "require": {
        "types": "./dist/utils/index.d.cts",
        "default": "./dist/utils/index.cjs"
      }
    },
    "./universal/index.cjs": "./dist/universal/index.cjs",
    "./win/index.cjs": "./dist/win/index.cjs",
    "./mac/index.cjs": "./dist/mac/index.cjs",
    "./utils/index.cjs": "./dist/utils/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "eagle",
    "eagle.cool",
    "eagle-plugin",
    "utilities",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "@eslint/js": "^9.0.0",
    "typescript-eslint": "^8.0.0",
    "prettier": "^3.4.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZackaryW/eagle-cooltils.git"
  }
}
